<%= @room.number %>
<br>
<iframe width="560" height="315" src=<%= "//www.youtube.com/embed/#{@link}" %> frameborder="0" allowfullscreen></iframe>  


<% if @users.present? %> 

    <%= form_with(model: @user, url: room_invited_path(room_id: @room.id), method: :post) do |f| %>
        <%= f.label :username %>
        <%= f.collection_select(:id, @users, :id, :user_form) %>

        <%= f.submit 'Criar Sala' %>
    <% end %>
<% end %> 


